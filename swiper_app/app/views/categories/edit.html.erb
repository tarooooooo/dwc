<div class="container">
  <div class="row">
    <div class="col-md-3">
    <h3>カテゴリー編集</h3>
      <%= form_with model: @category, local: true do |f| %>
        <%= f.label :name, 'カテゴリー名'%>
        <%= f.text_field :name ,class:'form^control' %>
        <%= f.submit '登録', class:'btn' %>
      <% end %>
      
    </div>
  </div>
</div>